//no1
function lampulalulitas(lampu){
    if (lampu == `merah` ||lampu == `merah`){
        console.log(`berhenti`);
    }
    else if (lampu == `kuning` ||lampu == `kuning`){
        console.log(`hati-hati`);
    }
    else if (lampu == `hijau` ||lampu == `hijau`){
        console.log(`maju`);
    }
}
lampulalulitas(`hijau`);